function addQSParm(myUrl,name,value){var re=new RegExp("([?&]"+name+"=)[^&]+","");function add(sep){myUrl+=sep+name+"="+encodeURIComponent(value)}
function change(){myUrl=myUrl.replace(re,"$1"+encodeURIComponent(value))}
if(myUrl.indexOf("?")===-1){add("?")}else{if(re.test(myUrl)){change()}else{add("&")}}
return myUrl}
