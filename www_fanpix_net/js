if(last_active.length){last_active.removeClass('xactive')}
container.addClass('xactive');var pos=-1;var topic_cells=ulcontainer.find('>li:not(.gapli)');topic_cells.each(function(index){if($(this).hasClass('xactive')){pos=index}});last_pos=topic_cells.length-1;var items_per_row=1;if($('#viewType').val()==3){items_per_row=8;var li_data=!1;var li_class='gapli arrow_box2 abpos';var adjfactor=-50}else if($('#viewType').val()==4){items_per_row=4;var li_data=!0;var li_class='gapli arrow_box2 xbox2 scard abcpos';var adjfactor=30}
var row=Math.floor(pos/items_per_row);var col=pos%items_per_row+1;li_class=li_class+col;if(!last_active.length||!$('#divConnection').length||(last_active.offset().top!=container.offset().top)){var insert_after_pos=items_per_row*(row+1)-1
if(insert_after_pos>last_pos){insert_after_pos=last_pos}
$('#divConnection').remove();ulcontainer.find('>li').eq(insert_after_pos).after('<li id="divConnection" class="'+li_class+'" style="width:100%;"></li>');$("#divConnection").height(0).html($('#loadDiv').html());addConnection(idf,null,li_data);$('#divConnection').animate({height:315},300,function(){$('#divConnection').css({"min-height":"315px","height":"auto"})})}else{$("#divConnection").attr('class',li_class);$("#divConnection").height($("#divConnection").height()).html($('#loadDiv').html());addConnection(idf,null,li_data)}
scrollhere(container,adjfactor)}
