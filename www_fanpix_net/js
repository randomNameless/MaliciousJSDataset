if(main_pid==0){if(first_li.length){first_li.addClass('highimg');imgStage(first_li,max_width);if(first_li.length){container.find('.add_img').hide()}
animateResize(img_stage,max_width,max_height)}}else{imgset.find('li[data-pid="'+main_pid+'"]').addClass('highimg')}}});animateResize(img_stage,max_width,max_height);gl.show()}else{nextStagePhoto(container.find('.img_click'),max_width,max_height)}}
function openConnection(idf){if($('#tabDiv').html()==''){var active_zone=$('#xZone').find('li.zactive');if(active_zone.length){active_zone.removeClass('zactive')}
