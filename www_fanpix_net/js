function validateJoinForm(){if(Trim(document.joinForm.webname.value)==''){alert('Please enter a valid webname');document.joinForm.webname.focus()}
else if(Trim(document.joinForm.email.value)==''){alert('Please enter a valid email');document.joinForm.email.focus()}
else if(document.joinForm.email2.value!=document.joinForm.email.value){alert("Please confirm your email")
document.joinForm.email2.focus()}
else if(Trim(document.joinForm.password.value)==''){alert('Please enter a valid password');document.joinForm.password.focus()}
else if(document.joinForm.password2.value!=document.joinForm.password.value){alert("Please confirm your password")
document.joinForm.password2.focus()}
else if(Trim(document.joinForm.location.value)==''){alert('Please enter the name of your city/town');document.joinForm.location.focus()}
else if(document.joinForm.country_id[0].selected){alert("Please select your country")
document.joinForm.country_id.focus()}
else if(document.joinForm.gender[0].selected){alert("Please select your gender")
document.joinForm.gender.focus()}
else if(document.joinForm.date_day[0].selected){alert("Please select your day of birth")
document.joinForm.date_day.focus()}
else if(document.joinForm.date_month[0].selected){alert("Please select your month of birth")
document.joinForm.date_month.focus()}
else if(document.joinForm.date_year[0].selected){alert("Please select your year of birth")
document.joinForm.date_year.focus()}
else{document.joinForm.submit()}}
