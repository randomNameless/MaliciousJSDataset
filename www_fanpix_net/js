function imgResize(iw,ih,maxw,maxh){if(!maxh){maxh=maxw*3/4}
if(iw>maxw){ih=ih*maxw/iw;iw=maxw}
if(ih>maxh){iw=iw*maxh/ih;ih=maxh}
return{iw:iw,ih:ih}}
function animateResize(imgt,maxw,maxh){var iw=imgt.attr('width');var ih=imgt.attr('height');if(iw>0){if(!maxh){maxh=maxw*3/4}
var i=imgResize(iw,ih,maxw,maxh);var imgt_div=imgt.closest('div')
$(function(){$(imgt).animate({width:i.iw,height:i.ih},{duration:300,queue:!1});$(imgt_div).animate({width:maxw,height:maxh},{duration:300,queue:!1})})}}
function closeGallery(closeli,remove_image){if(closeli.length){closeli.removeClass('xactive');closeli.find('.opengl').html('');var small_width=250;var img_stage=closeli.find('.bigimg');if(remove_image){closeli.find('.add_img').show();img_stage.hide();img_stage.attr('data-pid',0);img_stage.closest('div').css('height','auto').css('width',small_width)}else{animateResize(img_stage,small_width)}}}
