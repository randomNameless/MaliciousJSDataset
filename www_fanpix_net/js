function setNavImg(container){var selected_li=container.find('li.highimg');var ulcontainer=selected_li.parent();var pos=selected_li.index();var last_pos=ulcontainer.children().length-1;$(".prevImg").removeClass('hide');$(".nextImg").removeClass('hide');if(pos==0){$(".prevImg").addClass('hide')}else if(pos==last_pos){$(".nextImg").addClass('hide')}}
function loadimg(idf){$(".cbimg_bg").hide();$(".cbimg").attr("src",'');var iw=$(idf).data('w');var ih=$(idf).data('h');var id=$(idf).data('id');var os=$(idf).data('os');var hs=$(idf).data('hs');if(iw>0){$.colorbox.resize({height:ih+'px'});$('.imgbox').height(ih);$('.txtbox').height(ih);if($(idf).length){var urlstr=$(idf).attr("src");var urlRegex=/\/[a-z0-9]\/[a-z0-9]\/([a-z0-9]{12})/;var str=urlstr.match(urlRegex)[1]}else{var str=$(idf).data('str')}
if(hs.length){$(".cbimg_bg").width(iw).height(ih).css({'background-image':'url('+get_img_url(str+os,'orig')+')','background-position':'0px 0px','background-size':'cover','padding':'1px 0px 0px 1px'});var src=get_img_url(str+hs,'huge')}else{$(".cbimg_bg").width('auto').height('auto').css({'background':'none','padding':'0px'});var src=get_img_url(str+os,'orig')}
$(".cbimg").attr("width",iw);$(".cbimg").attr("height",ih);$(".cbimg").attr("src",src);$(".cbimg").attr("data-id",id);$(".cbimg_bg").show();var li_element=$(idf).closest('li');if(li_element.hasClass('swiper-slide')){var container=$('.sw_contain:last');container.find('li.highimg').removeClass("highimg");li_element.addClass("highimg");setNavImg(container)}}}
function clickimg(idf){var container=$('.sw_contain:last');var selected_li=container.find('li.highimg');if(selected_li.length){navimg(idf,'next')}else{}}
function navimg(idf,next_prev){var mySwiper=$('.sw_contain .swiper-container')[1].swiper;var container=$('.sw_contain:last');var selected_li=container.find('li.highimg');var ulcontainer=selected_li.parent();var pos=selected_li.index();var first_pos=0;var last_pos=ulcontainer.children().length-1;var next_pos=0;if(next_prev=='next'){if(pos!=last_pos){next_pos=pos+1;mySwiper.slideNext()}else{next_pos=first_pos;mySwiper.slideTo(first_pos)}}else{if(pos!=first_pos){next_pos=pos-1;mySwiper.slidePrev()}else{next_pos=last_pos;mySwiper.slideTo(last_pos)}}
