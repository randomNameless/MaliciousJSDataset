var ThumbsWidget=function(PID,TBL,SID,options,stats){this.VOTES={"THUMBS_UP":1,"THUMBS_DOWN":2,"THUMBS_NONE":3};this.vote=3;this.initialize(PID,TBL,SID,options,stats)}
ThumbsWidget.prototype.initialize=function(PID,TBL,SID,options,stats){this.PID=PID;this.TBL=TBL;this.SID=SID;this.setOptions(options,stats);this.star=$("#"+this.options.widget+" .star");this.message_text=$("#"+this.options.widget+" .message_text");this.up=$("#"+this.options.widget+' .up');this.down=$("#"+this.options.widget+' .down');this.score=$("#"+this.options.widget+' .score');this.votes=$("#"+this.options.widget+' .votes');this.up.bind('click',$.proxy(this.handleUp,this));this.down.bind('click',$.proxy(this.handleDown,this))},ThumbsWidget.prototype.setOptions=function(options,stats){this.options={widget:'thumbs'};this.stats={score:0,votes:0};this.options=options;this.stats=stats},ThumbsWidget.prototype.handleUp=function(){if(currentUser.id==''){alert("Login or register to vote!");return}
switch(this.vote){case this.VOTES.THUMBS_UP:this.vote=this.VOTES.THUMBS_NONE;this.stats.score-=1;this.stats.votes-=1;break;case this.VOTES.THUMBS_DOWN:this.vote=this.VOTES.THUMBS_UP;this.stats.score+=1;break;case this.VOTES.THUMBS_NONE:this.vote=this.VOTES.THUMBS_UP;this.stats.score+=1;this.stats.votes+=1;break}
this.paint();var params="PID="+this.PID+"&TBL="+this.TBL+"&SID="+this.SID+"&vote="+this.vote;$.ajax({url:ajaxDomain+'/include/actions/add_thumb.asp',data:params,xhrFields:{withCredentials:!0},async:!0})},ThumbsWidget.prototype.handleDown=function(){if(currentUser.id==''){alert("Login or register to vote!");return}
switch(this.vote){case this.VOTES.THUMBS_UP:this.vote=this.VOTES.THUMBS_DOWN;this.stats.score-=1;break;case this.VOTES.THUMBS_DOWN:this.vote=this.VOTES.THUMBS_NONE;this.stats.votes-=1;break;case this.VOTES.THUMBS_NONE:this.vote=this.VOTES.THUMBS_DOWN;this.stats.votes+=1;break}
this.paint();var params="PID="+this.PID+"&TBL="+this.TBL+"&SID="+this.SID+"&vote="+this.vote;$.ajax({url:ajaxDomain+'/include/actions/add_thumb.asp',data:params,xhrFields:{withCredentials:!0},async:!0})},ThumbsWidget.prototype.paint=function(){this.up.removeClass("on");this.down.removeClass("on");this.up.removeClass("off");this.down.removeClass("off");this.message_text.text("VOTE COUNTED");this.up.attr("style","visibility: hidden");this.down.attr("style","visibility: hidden");switch(this.vote){case this.VOTES.THUMBS_UP:this.up.removeClass("off");this.down.removeClass("on");this.up.addClass("on");this.down.addClass("off");this.star.html("<img src="+staticDomain+"/img/icons/stars/10_star.png>");break;case this.VOTES.THUMBS_DOWN:this.up.removeClass("on");this.down.removeClass("off");this.up.addClass("off");this.down.addClass("on");this.star.html("<img src="+staticDomain+"/img/icons/stars/0_star.png>");break;case this.VOTES.THUMBS_NONE:this.up.removeClass("on");this.down.removeClass("on");this.up.addClass("off");this.down.addClass("off");break}
if(this.stats.score>=0){this.score.html("")}else{this.score.html("")}
if(this.stats.score==1){this.votes.html(""+this.stats.votes+" vote")}else{this.votes.html(""+this.stats.votes+" votes")}};var FriendsWidget=function(MDR,PID){var params="MDR="+MDR+"&PID="+PID;$.ajax({url:ajaxDomain+'/include/actions/add_friend.asp',data:params,xhrFields:{withCredentials:!0},async:!0,success:function(data){$("#FriendsDiv"+PID).html(data)}})};var FavWidget=function(PID,TBL,SID){var params="PID="+PID+"&TBL="+TBL+"&SID="+SID;$.ajax({url:ajaxDomain+'/include/actions/add_favourite.asp',data:params,xhrFields:{withCredentials:!0},async:!0,success:function(data){$("#FavDiv"+PID).html(data)}})};var FavdelWidget=function(PID,TBL,SID,DKY){var params="PID="+PID+"&TBL="+TBL+"&SID="+SID+"&DKY="+DKY;$.ajax({url:ajaxDomain+'/include/actions/delete_favourite.asp',data:params,xhrFields:{withCredentials:!0},async:!0,success:function(data){$("#FavdelDiv"+PID).html(data)}})};var FlagWidget=function(PID,TBL){var params="PID="+PID+"&TBL="+TBL;$.ajax({url:ajaxDomain+'/include/actions/flag_entry.asp',data:params,xhrFields:{withCredentials:!0},async:!0,success:function(data){$("#FlagDiv"+PID).html(data)}})};function validate_commentForm(){if(document.commentForm.postname.value==''&&currentUser.id==0){alert('Please enter your name')
document.commentForm.postname.focus()}
else if(document.commentForm.email.value==''&&currentUser.id==0){alert('Please enter your email address')
document.commentForm.email.focus()}
else if(document.commentForm.comment.value.length<4){alert('Please enter your comment')
document.commentForm.comment.focus()}
else if(document.commentForm.comment.value.length>1000){alert('Please ensure your comment is less than 1000 characters')
document.commentForm.comment.focus()}
else if(document.commentForm.comment.value.indexOf("<")!=-1){alert('Please do not use the "<" character')
document.commentForm.comment.focus()}
else if(document.commentForm.comment.value.indexOf(">")!=-1){alert('Please do not use the ">" character')
document.commentForm.comment.focus()}
else if(document.commentForm.postname.value.indexOf("<")!=-1){alert('Please do not use the "<" character')
document.commentForm.postname.focus()}
else if(document.commentForm.postname.value.indexOf(">")!=-1){alert('Please do not use the ">" character')
document.commentForm.postname.focus()}
else{document.commentForm.add_comment_button.value="Adding comment...";document.commentForm.add_comment_button.disabled=!0;comment=document.commentForm.comment.value.replace(/ /g,'+');comment=comment.replace(/\n/g,"[[br]]");comment=comment.replace("&","%26");postname=document.commentForm.postname.value.replace(/ /g,'+');email=document.commentForm.email.value;website=document.commentForm.website.value;TBL=document.commentForm.TBL.value;PID=document.commentForm.PID.value;SID=document.commentForm.SID.value;var params="postname="+postname+"&comment="+comment+"&email="+email+"&website="+website+"&TBL="+TBL+"&PID="+PID+"&SID="+SID;$.ajax({url:ajaxDomain+'/include/actions/add_comment.asp',data:params,xhrFields:{withCredentials:!0},async:!0,success:function(data){$("#serverData").html(data);document.commentForm.add_comment_button.value="Comment Posted!";document.commentForm.add_comment_button.disabled=!0}})}}
